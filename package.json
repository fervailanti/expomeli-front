{
  "name": "expomeli-front",
  "version": "0.1.0",
  "description": "Web app for searching MercadoLibre products",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "autoprefixer": "^10.0.1",
    "axios": "^0.20.0",
    "query-string": "^6.13.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "tailwindcss": "^1.9.2"
  },
  "scripts": {
    "start": "yarn build:css && react-scripts start",
    "build:css": "tailwindcss build src/styles/tailwind.css -o src/styles/build.css",
    "build:dev": "yarn build:css && react-scripts build",
    "build:prod": "NODE_ENV=production yarn build:css && react-scripts build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "stylelint-config-recommended": "^3.0.0"
  }
}
